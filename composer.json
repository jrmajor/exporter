{
    "name": "jrmajor/exporter",
    "description": "Like var_export, but unstable and you shouldn't use it",
    "license": "MIT",
    "keywords": [
        "var_export",
        "serializer"
    ],
    "authors": [
        {
            "name": "Jeremiasz Major",
            "email": "jrh.mjr@gmail.com"
        }
    ],
    "homepage": "https://github.com/jrmajor/exporter",
    "require": {
        "php": "8.1 - 8.2",
        "azjezz/psl": "^2.0"
    },
    "require-dev": {
        "jrmajor/cs": "^0.5.1",
        "php-standard-library/psalm-plugin": "^2.2",
        "phpbench/phpbench": "^1.2",
        "phpunit/phpunit": "^10.2",
        "psalm/plugin-phpunit": "^0.18.4",
        "symfony/var-dumper": "^6.3",
        "vimeo/psalm": "^5.13"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Major\\Exporter\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Major\\Exporter\\Tests\\": "tests"
        }
    }
}
