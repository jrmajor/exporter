{
    "name": "jrmajor/exporter",
    "description": "Like var_export, but unstable and you shouldn't use it",
    "license": "MIT",
    "keywords": [
        "var_export",
        "serializer"
    ],
    "authors": [
        {
            "name": "Jeremiasz Major",
            "email": "jrh.mjr@gmail.com"
        }
    ],
    "homepage": "https://github.com/jrmajor/exporter",
    "require": {
        "php": "8.2 - 8.3",
        "azjezz/psl": "^2.9"
    },
    "require-dev": {
        "jrmajor/cs": "^0.6.0",
        "php-standard-library/phpstan-extension": "^2.0",
        "phpbench/phpbench": "^1.3",
        "phpstan/phpstan": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpunit/phpunit": "^11.2",
        "symfony/var-dumper": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Major\\Exporter\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Major\\Exporter\\Tests\\": "tests"
        }
    }
}
